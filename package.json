{
  "name": "create-fullstack-app-js",
  "version": "0.0.1",
  "description": "A CLI that creates a fullstack boilerplate application.",
  "bin": {
    "create-full-stack-app-js": "bin/create-fullstack-app-js",
    "@shubhankar_kg/create-full-stack-app-js": "bin/create-fullstack-app-js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "mocha 'tests/**/*.ts'",
    "coverage": "nyc npm test",
    "build": "tsc",
    "lint": "eslint . --ext .ts --fix",
    "format": "prettier . --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint",
      "npm run format"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShubhankarKG/create-fullstack-app-js.git"
  },
  "keywords": [
    "fullstack",
    "templates",
    "nodejs"
  ],
  "files": [
    "bin",
    "dist",
    "templates"
  ],
  "author": "ShubhankarKG",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ShubhankarKG/create-fullstack-app-js/issues"
  },
  "homepage": "https://github.com/ShubhankarKG/create-fullstack-app-js#readme",
  "dependencies": {
    "arg": "^5.0.0",
    "chalk": "^4.1.0",
    "execa": "^5.0.0",
    "inquirer": "^7.3.3",
    "listr": "^0.14.3",
    "mkdirp": "^1.0.4",
    "ncp": "^2.0.0",
    "pkg-install": "^1.0.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.18",
    "@types/execa": "^2.0.0",
    "@types/inquirer": "^7.3.1",
    "@types/listr": "^0.14.2",
    "@types/mkdirp": "^1.0.1",
    "@types/mocha": "^8.2.2",
    "@types/ncp": "^2.0.4",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "eslint": "^7.25.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "mocha": "^8.4.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}